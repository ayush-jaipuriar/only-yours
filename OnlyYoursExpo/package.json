{
  "name": "onlyyoursexpo",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=24 <25",
    "npm": ">=11 <12"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@stomp/stompjs|axios)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "src/screens/__tests__/"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/stack": "^7.7.2",
    "@stomp/stompjs": "^7.3.0",
    "axios": "^1.13.5",
    "expo": "~54.0.33",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "private": true,
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.17"
  }
}
